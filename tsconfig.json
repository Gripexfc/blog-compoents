{
    "compilerOptions": {
      "target": "es5",
      "module": "commonjs",
      "jsx": "react",
      "strict": true,
      "esModuleInterop": true,
      "declaration": true,
      "outDir": "./dist",
      "rootDir": "./src"
    },
    "include": [
        "../stories/**/*.mdx",
        "../stories/**/*.stories.@(js|jsx|mjs|ts|tsx)",
        "../components/**/*.mdx",
        "../components/**/*.stories.@(js|jsx|mjs|ts|tsx)",
    ],
    "exclude": ["node_modules", "dist"]
  }
  